   "source": [
    "import cv2 \n",
    "import numpy as np \n",
    "img=cv2.imread(\"klon.jpg\",0)\n",
    "\n",
    "row , col =img.shape\n",
    "x_bosluk=100\n",
    "y_bosluk=100\n",
    "\n",
    "M=np.float32([[1,0,x_bosluk],[0,1,y_bosluk]])\n",
    "\n",
    "dst=cv2.warpAffine(img,M,(row ,col))\n",
    "\n",
    "\n",
    "cv2.imshow(\"s\",dst)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n",
    "\n",
    "\n",
    "\n"
   ]
  }

    "import numpy as np \n",
    "img=cv2.imread(\"klon.jpg\")\n",
    "\n",
    "blur=cv2.blur(img,(5,5))\n",
    "blur2=cv2.GaussianBlur(img,(5,5),cv2.BORDER_DEFAULT)\n",
    "cv2.imshow(\"org\",img)\n",
    "cv2.imshow(\"1\",blur)\n",
    "cv2.imshow(\"2\",blur2)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n",
    "\n"

    "import cv2\n",
    "import numpy as np\n",
    "img=cv2.imread(\"klon.jpg\",0)\n",
    "\n",
    "\n",
    "\n",
    "kernel=np.ones((5,5),np.uint8)\n",
    "\n",
    "erosion=cv2.erode(img,kernel,iterations=1)\n",
    "\n",
    "\n",
    "\n",
    "cv2.imshow(\"1\", img)\n",
    "cv2.imshow(\"2\", erosion)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
  }
